@model TallerAPI.Data.Entities.VehicleType

@{
    ViewData["Title"] = "Create";
}

<h1>crear</h1>

<h4>Tipo de vehiculo</h4>
<hr />
<div class="row">
    <div class="col-md-12">
        <form asp-action="Create">            
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>

            <partial name="_VehicleType" />

            <div class="form-group">
                <input type="submit" value="crear" class="btn btn-outline-primary" />
                <a asp-action="Index" class="btn btn-outline-success">Regresar</a>
            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
